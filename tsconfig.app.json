{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "main.ts"],
  "exclude": ["src/**/__tests__/*","node_modules","dist"],
  "compilerOptions": {
    "outDir":"./",
    "esModuleInterop": true,
    "composite": true,
    "baseUrl": ".",
    "moduleResolution":"node",
    "paths": {
      "@/*": ["./src/*"]
    },
    "resolveJsonModule":false
  },
}